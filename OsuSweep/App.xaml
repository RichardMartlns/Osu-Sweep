<Application x:Class="OsuSweep.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:localization="clr-namespace:OsuSweep.Services.Localization"
             xmlns:local="clr-namespace:OsuSweep">

    <Application.Resources>

        <ResourceDictionary>
            <localization:LocalizationProvider x:Key="Locale" />

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/ComboBoxStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>


    </Application.Resources>
</Application>
